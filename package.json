{
  "name": "@kalisio/krawler",
  "description": "Make automated process of extracting data from web services easy",
  "version": "0.7.0",
  "homepage": "https://github.com/kalisio/krawler",
  "main": "lib/",
  "bin": "lib/index.js",
  "keywords": [
    "feathers",
    "feathers-plugin",
    "etl",
    "gis"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kalisio/krawler.git"
  },
  "author": {
    "name": "Kalisio contributors",
    "url": "https://github.com/kalisio"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/kalisio/krawler/issues"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "publish": "git push origin --tags && npm run changelog && git push origin",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "changelog": "github_changelog_generator -u kalisio -p krawler && git add CHANGELOG.md && git commit -am \"Updating changelog\"",
    "compile": "shx rm -rf lib/ && babel -d lib/ --copy-files src/",
    "watch": "babel --watch -d lib/ --copy-files src/",
    "lint": "standard src/**/*.js test/**/*.js --fix",
    "mocha": "mocha --opts mocha.opts",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --opts mocha.opts",
    "test": "npm run compile && npm run lint && npm run coverage",
    "start": "npm run compile && node example/app",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "standard": {
    "parser": "babel-eslint",
    "sourceType": "module",
    "env": [
      "mocha"
    ]
  },
  "pkg": {
    "scripts": [
      "lib/**/*.js"
    ]
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "@mapbox/sphericalmercator": "^1.0.5",
    "aws-sdk": "^2.158.0",
    "body-parser": "^1.17.1",
    "commander": "^2.11.0",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "cron": "^1.3.0",
    "debug": "^3.1.0",
    "dockerode": "^2.5.5",
    "fast-csv": "^2.4.1",
    "feathers": "^2.2.3",
    "feathers-errors": "^2.9.2",
    "feathers-hooks": "^2.1.2",
    "feathers-hooks-common": "^3.0.0",
    "feathers-rest": "^1.7.1",
    "feathers-socketio": "^2.0.0",
    "feathers-sync": "^0.1.4",
    "fs-blob-store": "^5.2.1",
    "fs-extra": "^4.0.2",
    "gdal": "^0.9.6",
    "geo-pixel-stream": "^0.4.0",
    "helmet": "^3.5.0",
    "js-yaml": "^3.10.0",
    "json2csv": "^3.11.4",
    "kue": "^0.11.6",
    "lodash": "^4.17.10",
    "mathjs": "^4.1.2",
    "memory-blob-store": "^5.0.1",
    "merge-stream": "^1.0.1",
    "moment": "^2.22.1",
    "mongodb": "^3.0.10",
    "pg": "^7.4.0",
    "proj4": "^2.4.4",
    "reproject": "^1.2.1",
    "request": "^2.83.0",
    "s3-blob-store": "^2.1.0",
    "sift": "^5.1.0",
    "snyk": "^1.83.0"
    "tar": "^4.4.4",
    "uuid": "^3.2.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^3.5.0",
    "chai-lint": "^0.1.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.5.3",
    "nock": "^9.2.6",
    "shx": "^0.2.2",
    "standard": "^10.0.3",
    "vuepress": "^0.10.1",
    "vuepress-theme-kalisio": "https://github.com/kalisio/vuepress-theme-kalisio"
  },
  "snyk": true
}
